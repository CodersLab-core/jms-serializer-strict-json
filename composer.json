{
    "name": "justblackbird/jms-serializer-strict-json",
    "type": "library",
    "description": "Provide strict JSON deserialization",
    "version": "0.2.0",
    "keywords": [
        "jms",
        "serializer",
        "json"
    ],
    "homepage": "https://github.com/JustBlackBird/jms-serializer-strict-json",
    "license": "Apache2",
    "authors": [
        {
            "name": "Dmitriy Simushev",
            "email": "simushevds@gmail.com"
        }
    ],
    "require": {
        "jms/serializer": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "squizlabs/php_codesniffer": "^2.8"
    },
    "scripts": {
        "test": ["phpunit", "phpcs"]
    },
    "autoload": {
        "psr-4": {
            "JustBlackBird\\JmsSerializerStrictJson\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JustBlackBird\\JmsSerializerStrictJson\\Tests\\": "tests"
        }
    }
